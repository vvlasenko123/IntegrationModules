{
  "name": "integration-modules",
  "private": false,
  "version": "0.0.4",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./note":    "./dist/widgets/NoteBoardWidget.js",
    "./emoji":   "./dist/widgets/EmojiBoardWidget.js",
    "./shape":   "./dist/widgets/ShapeBoardWidget.js",
    "./markdown": "./dist/widgets/MarkdownBoardWidget.js",
    "./roadmap": "./dist/widgets/RoadmapBoardWidget.js"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --config vite.lib.config.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "vite": "^7.2.4"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@xyflow/react": "^12.6.0",
    "axios": "^1.13.2",
    "d3-drag": "^3.0.0",
    "d3-selection": "^3.0.0",
    "date-fns": "^4.1.0",
    "i18next": "^25.6.3",
    "konva": "^10.0.12",
    "re-resizable": "^6.11.2",
    "react": "^19.0.0",
    "react-datepicker": "^9.1.0",
    "react-dom": "^19.0.0",
    "react-draggable": "^4.5.0",
    "react-i18next": "^16.3.5",
    "react-konva": "^19.2.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.2",
    "remark-gfm": "^4.0.1",
    "vite-plugin-svgr": "^4.5.0",
    "zustand": "^5.0.8"
  }
}
